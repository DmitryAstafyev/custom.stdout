{
  "name": "custom.stdout",
  "version": "1.0.2",
  "description": "Allows split output in node stdout to areas. Helps to show \"static\" information in life-logs, like state of server/service and so on",
  "main": "./dist/stdout.js",
  "types": "./dist/stdout.d.ts",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "build:watch": "tsc -p ./tsconfig.json -w",
    "test:unit": "./node_modules/.bin/jasmine-ts",
    "tslint": "./node_modules/.bin/tslint -c tslint.json './src/**/*{.ts,.tsx}'"
  },
  "author": "Dmitry Astafyev (D.Astafyev@outlook.com)",
  "license": "Apache 2",
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "@types/node": "^10.12.0",
    "jasmine": "^3.3.0",
    "jasmine-core": "^3.3.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/DmitryAstafyev/custom.stdout.git"
  },
  "keywords": [
    "stdout",
    "console",
    "sticky",
    "fixed",
    "error"
  ]
}
